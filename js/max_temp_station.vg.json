{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "A simple bar chart with embedded data.",
  "data": {
      "url": "data/Australia_Rainfall.csv"
  },
  "mark": "bar",
  "encoding": {
    "y": {"field": "Station", "type": "nominal", "sort": {"field": "Max °C", "op": "average", "order":"descending"}},
    "x": {"aggregate":"max", "field": "Max °C", "type": "quantitative"}
  }
}

